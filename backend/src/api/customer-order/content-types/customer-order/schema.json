{
  "kind": "collectionType",
  "collectionName": "customer_orders",
  "info": {
    "singularName": "customer-order",
    "pluralName": "customer-orders",
    "displayName": "CustomerOrder"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "date": {
      "type": "datetime"
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "pending",
        "paid",
        "shipped",
        "completed",
        "cancelled"
      ]
    },
    "total": {
      "type": "decimal"
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "customer_order"
    }
  }
}
